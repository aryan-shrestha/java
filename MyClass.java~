class MyClass {
  public static void main(String[] args){
    int a = 8; 
    int b = 6; 
    int c = 2;
    
    if (a>b && a>c) {
      System.out.println("A is Greaetr!");
    } else if (b>a &&  b>c) {
      System.out.println("B is Greater!");
    } else {
      System.out.println("C is Greater!");
 
    }
  }
}